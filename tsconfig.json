{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "lib": [
      "dom",
      "es2017",
      "es2015.collection",
      "es2015.iterable"
    ],
    "outDir": "./tsout",
    "rootDir": "./",
    "strict": false,
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "downlevelIteration": true,
    "importHelpers": true
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "vendor"
  ],
  "include": [
    "redis",
    "src"
  ]
}
